# sort a tuple in ascending order without using sort() or sorted().
t = (4, 1, 3, 2)
lst = list(t)
n = len(lst)
for i in range(n):
    for j in range(0, n - i - 1):
        if lst[j] > lst[j + 1]:
            lst[j], lst[j + 1] = lst[j + 1], lst[j]
sorted_tuple = tuple(lst)
print(sorted_tuple)